{
  "name": "elm-debug-transformer",
  "version": "1.0.0-rc2",
  "description": "Transform Elm Debug.log output into nice log object with custom formatter",
  "main": "dist/elm-console-debug.js",
  "author": "Tomas Latal <tom@tomaslatal.cz>",
  "repository": "https://github.com/kraklin/elm-debug-transformer",
  "keywords": [
    "elm",
    "debug",
    "console",
    "parser",
    "formatter"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "pegjs": "pegjs --plugin ./node_modules/ts-pegjs/src/tspegjs -o src/elm-debug-parser.ts --cache src/elm-debug.pegjs",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --opts mocha.opts",
    "test:watch": "yarn test --watch",
    "build": "yarn run pegjs && webpack",
    "prepublishOnly": "yarn run test && yarn run build"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "chai": "^4.2.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.11",
    "mocha": "^6.1.4",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.4",
    "prettier": "^1.18.2",
    "raw-loader": "^3.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "ts-pegjs": "^0.2.5",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/pegjs": "^0.10.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true
  }
}
