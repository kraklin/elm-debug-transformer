{
  "name": "elm-debug-transformer",
  "version": "1.2.1",
  "description": "Transform Elm Debug.log output into nice log object with custom formatter",
  "author": "Tomas Latal <tom@tomaslatal.cz>",
  "repository": "https://github.com/kraklin/elm-debug-transformer",
  "type": "module",
  "keywords": [
    "elm",
    "debug",
    "console",
    "parser",
    "formatter"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "pegjs": "pegjs --plugin ./node_modules/ts-pegjs/src/tspegjs -o src/elm-debug-parser.ts src/elm-debug.pegjs",
    "test": "mocha --require ts-node/register --require jsdom-global/register",
    "build": "npm run pegjs && tsc && vite build",
    "build:watch": "npm run pegjs && tsc && vite build --watch",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/chai": "^4.3.5",
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.7",
    "chai": "^4.3.7",
    "elm-optimize-level-2": "^0.3.5",
    "elm-tooling": "^1.14.0",
    "elm-ts-interop": "^0.0.8",
    "elm-watch": "^1.1.2",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.15",
    "mocha": "^10.2.0",
    "pegjs": "^0.10.0",
    "prettier": "^3.0.1",
    "rollup-plugin-elm": "^3.0.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "ts-loader": "^6.0.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^8.10.2",
    "ts-pegjs": "^0.2.5",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-elm": "^2.8.0",
    "vite-plugin-peggy-loader": "^1.0.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true
  }
}
