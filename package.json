{
  "name": "elm-debug-transformer",
  "version": "1.2.1",
  "description": "Transform Elm Debug.log output into nice log object with custom formatter",
  "author": "Tomas Latal <tom@tomaslatal.cz>",
  "repository": "https://github.com/kraklin/elm-debug-transformer",
  "type": "module",
  "keywords": [
    "elm",
    "debug",
    "console",
    "parser",
    "formatter"
  ],
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "elm": "elm-esm make src/Parser.elm --output=src/elm-debug-parser.js",
    "test": "vitest run",
    "build": "npm run elm && vite build",
    "build:watch": "npm run elm && vite build --watch",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/lodash": "^4.14.134",
    "@types/node": "^20.4.7",
    "elm-esm": "^1.1.4",
    "elm-optimize-level-2": "^0.3.5",
    "elm-tooling": "^1.14.0",
    "elm-ts-interop": "^0.0.8",
    "elm-watch": "^1.1.2",
    "jsdom": "^22.1.0",
    "lodash": "^4.17.15",
    "prettier": "^3.0.1",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vitest": "^0.34.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true
  }
}
